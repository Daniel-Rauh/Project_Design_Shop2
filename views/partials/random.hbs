<section id="random">
    {{#each randomItems}}
        <a href="/single/{{_id}}" class="randomItem">
                <img src={{product_picture_link}} alt="">
                <div class="info">
                    <h2>{{product_name}}</h2>
                    <h2>${{price}}</h2>
                </div>
                <h3>{{company}}</h3>
        </a>
        {{/each}}
</section>